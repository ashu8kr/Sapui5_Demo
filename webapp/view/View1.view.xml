<mvc:View controllerName="com.sap.project1.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <VBox id="idMainVBox">
                <HBox id="idNameHBox">
                    <Label id="idNameLabel" text="Name"/>
                    <Input id="idNameInput"/>
                </HBox>
                <HBox id="idContactHBox">
                    <Label id="idContactLabel" text="Name"/>
                    <Input id="idContactInput"/>
                </HBox>
            </VBox>
        </content>
    </Page>
</mvc:View>
